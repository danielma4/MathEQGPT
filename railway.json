{
    "$schema": "https://railway.com/railway.schema.json",
    "build": {
      "builder": "NIXPACKS",
      "install": [
        "pip install -r requirements.txt"
      ],
      "runtime": "python3.10"
    },
    "deploy": {
      "runtime": "V2",
      "numReplicas": 1,
      "sleepApplication": false,
      "multiRegionConfig": {
        "us-west1": {
          "numReplicas": 1
        }
      },
      "restartPolicyType": "ON_FAILURE",
      "restartPolicyMaxRetries": 10
    }
  }